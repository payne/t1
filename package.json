{
  "name": "t1",
  "version": "0.0.1",
  "description": "Messing around with typescript and jest",
  "main": "index.js",
  "scripts": {
        "test": "jest --coverage",
        "test:watch": "jest --coverage --watchAll",
        "build": "tsc",
        "build:watch": "onchange 'src/**/*.ts' -- npm run build",
        "view:coverage": "serve coverage/lcov-report",
        "clean": "rm -rf coverage src/**/*.js src/**/*.map",
        "start": "node src/index.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^25.1.2",
    "@types/node": "^13.7.1",
    "jest": "^25.1.0",
    "ts-jest": "^25.2.0",
    "typescript": "^3.7.5"
  }
}
